#include <iostream>
using namespace std;

void MergeSort(int *A, int f, int l);
void Merge(int *A, int f, int l);

int main()
{
	set
}

void Merge(int *A, int f, int l)
{
	int m, st, fin, j;
	int *mas = new int[100];
	m = (f + l) / 2;
	st = f;
	fin = m + 1; 
	for (j = f; j <= l; j++)
	if ((st <= m) && ((f > l) || (A[st] < A[f])))
	{
		mas[j] = A[st];
		st++;
	}
	else
	{
		mas[j] = A[f];
		f++;
	}
	for (j = f; j <= l; j++)
		A[j] = mas[j];
	delete[]mas;
}

void MergeSort(int *A, int f, int l)
{
	{
		if (f < l)
		{
			MergeSort(A, f, (f + l) / 2);
			MergeSort(A, (f+l)/2+1, l);
			Merge(A, f, l);
		}
	}
}
